// Reverse a arraylist.
import java.util.ArrayList;

public class ReverseArray_List
{
	public static void reverse(ArrayList<Integer> list)
	{
		int i = 0, j = list.size() - 1;

		while(i < j) {
			
			/* int temp = a;
			a = b;
			b = temp;
			*/

		Integer temp = Integer.valueOf(list.get(i));
		list.set(i, list.get(j));
		list.set(j, temp);
		i++;
		j--;
		}

	}
}