// 2nd Method 
public class FibinacciDemo2
{
	public static int fib(int n)
	{
		if (n == 0 || n == 1)
		{
			return n;
		}

		return fib(n-1) + fib(n-2);
	}

	public static void main(String[] args)
	{
		for (int i = 0; i <= 20; i++)
		{
			System.out.println(fib(i));
		}
		System.out.println(fib(6));
	}
}